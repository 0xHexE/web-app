{
  "root": "apps/workflow-webapp",
  "sourceRoot": "apps/workflow-webapp/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/workflow-webapp",
        "index": "apps/workflow-webapp/src/index.html",
        "baseHref": "/",
        "main": "apps/workflow-webapp/src/main.tsx",
        "polyfills": "apps/workflow-webapp/src/polyfills.ts",
        "tsConfig": "apps/workflow-webapp/tsconfig.app.json",
        "assets": [
          "apps/workflow-webapp/src/favicon.ico",
          "apps/workflow-webapp/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/workflow-webapp/src/environments/environment.ts",
              "with": "apps/workflow-webapp/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "workflow-webapp:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "workflow-webapp:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/workflow-webapp/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/workflow-webapp"],
      "options": {
        "jestConfig": "apps/workflow-webapp/jest.config.js",
        "passWithNoTests": true
      }
    },
    "intl": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "formatjs extract \"apps/workflow-webapp/src/**/*.{ts,tsx,vue}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file apps/workflow-webapp/src/locales/en-US.json",
          "formatjs compile-folder --ast --format simple \"apps/workflow-webapp/src/locales\" apps/workflow-webapp/src/assets/locales"
        ]
      }
    }
  },
  "tags": []
}
